[{"id_": "1250308940534960134", "raw_data": {"created_at": "Wed Apr 15 06:24:15 +0000 2020", "id": 1250308940534960134, "id_str": "1250308940534960134", "full_text": "Donald Trump stoppt die Zahlungen der USA an die Weltgesundheitsorganisation (WHO). Das ist unverantwortlich. Verharmlost hat er selbst das Corona-Virus. Nun schiebt er der WHO die Schuld zu. Ablenkungsmanöver eines Wahlkämpfers. Durchsichtig!", "truncated": false, "display_text_range": [0, 243], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 1, "favorite_count": 3, "reply_count": 1, "quote_count": 0, "conversation_id": 1250308940534960134, "conversation_id_str": "1250308940534960134", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1263390102195253249", "raw_data": {"created_at": "Thu May 21 08:44:07 +0000 2020", "id": 1263390102195253249, "id_str": "1263390102195253249", "full_text": "Der kirchenkritische Beitrag von Christine Lieberknecht in der „Welt“ wird heftig kritisiert. Von mir nicht! Sie hat recht! In viele Themen mischt sich die EKD lautstark ein. In der Corona-Krise war sie leise, sehr leise! Vertane Chance!", "truncated": false, "display_text_range": [0, 237], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 2, "favorite_count": 4, "reply_count": 0, "quote_count": 0, "conversation_id": 1263390102195253249, "conversation_id_str": "1263390102195253249", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1264146524054990848", "raw_data": {"created_at": "Sat May 23 10:49:52 +0000 2020", "id": 1264146524054990848, "id_str": "1264146524054990848", "full_text": "Die Kultur darf nicht Verlierer der Corona-Krise werden. Insbesondere die selbstständige n Küstlerinnen und Künstler haben große Einbußen ihrer Einnahmen. Auch freie Spielstätten oder auch Kinos leiden massiv unter den Einschränkungen. Da gibt es noch Nachsteuerungsbedarf.", "truncated": false, "display_text_range": [0, 273], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 6, "favorite_count": 24, "reply_count": 2, "quote_count": 0, "conversation_id": 1264146524054990848, "conversation_id_str": "1264146524054990848", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1286883295305904128", "raw_data": {"created_at": "Sat Jul 25 04:37:41 +0000 2020", "id": 1286883295305904128, "id_str": "1286883295305904128", "full_text": "Verpflichtende Corona-Tests bei der Einreise aus Risikoländern sind das Gebot der Stunde. Ich hoffe, dass ein solcher Beschluss so schnell wie möglich kommt. Die Freiwilligkeit ist gut gemeint, aber zu wenig effektiv.", "truncated": false, "display_text_range": [0, 217], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 1, "favorite_count": 5, "reply_count": 0, "quote_count": 1, "conversation_id": 1286883295305904128, "conversation_id_str": "1286883295305904128", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1315622581085626370", "raw_data": {"created_at": "Mon Oct 12 11:57:21 +0000 2020", "id": 1315622581085626370, "id_str": "1315622581085626370", "full_text": "Das Beherbergungsverbot muß fallen! Der Nutzen und die Verhältnismäßigkeit ist zweifelhaft, der Frust ist groß!", "truncated": false, "display_text_range": [0, 111], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 2, "favorite_count": 4, "reply_count": 0, "quote_count": 0, "conversation_id": 1315622581085626370, "conversation_id_str": "1315622581085626370", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1323296238385717248", "raw_data": {"created_at": "Mon Nov 02 16:09:43 +0000 2020", "id": 1323296238385717248, "id_str": "1323296238385717248", "full_text": "Einige Branchen werden unter dem zweiten Lockdown extrem leiden. Dazu gehört u.a. die Gastronomie, die Veranstaltungswirtschaft und die Kultur. Als Sprecherin für Kultur kann ich nur hoffen, dass sie bei den Hilfspaketen wirksam berücksichtigt wird.", "truncated": false, "display_text_range": [0, 249], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888, "user_id_str": "1135510177292197888", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 2, "favorite_count": 12, "reply_count": 1, "quote_count": 1, "conversation_id": 1323296238385717248, "conversation_id_str": "1323296238385717248", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1323565319131115521", "raw_data": {"created_at": "Tue Nov 03 09:58:57 +0000 2020", "id": 1323565319131115521, "id_str": "1323565319131115521", "full_text": "Künstler, Kreative und Freischaffende sind in Not. Kultur braucht Hilfe. Nicht nur im\nNovember. Die Gelder müssen auf Grundlage des Jahresdurchschnittseinkommens 2019 auch in dem Folgemonaten der Pandemie fließen.", "truncated": false, "display_text_range": [0, 213], "entities": {"hashtags": [], "symbols": [], "user_mentions": [], "urls": []}, "source": "<a href=\"http://twitter.com/download/iphone\" rel=\"nofollow\">Twitter for iPhone</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 1135510177292197888,
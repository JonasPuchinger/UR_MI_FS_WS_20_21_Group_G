uoted_status_permalink": {"url": "https://t.co/Aghmhjsw1L", "expanded": "https://twitter.com/Die_Insider/status/1321772552809816069", "display": "twitter.com/Die_Insider/st…"}, "retweet_count": 22, "favorite_count": 159, "reply_count": 10, "quote_count": 5, "conversation_id": 1322099494595547136, "conversation_id_str": "1322099494595547136", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1326936924393779201", "raw_data": {"created_at": "Thu Nov 12 17:16:30 +0000 2020", "id": 1326936924393779201, "id_str": "1326936924393779201", "full_text": "Trotz angespannter #Haushaltslage darf es jetzt nicht zu Einsparungen am falschen Ende kommen. Weitere #Corona-Gelder müssen fließen - bereits gestellte Anträge schnellstmöglich bearbeitet werden, um Hilfe leisten zu können. @olafscholz @spdbt @stimmeonline #Steuerschätzung", "truncated": false, "display_text_range": [0, 274], "entities": {"hashtags": [{"text": "Haushaltslage", "indices": [19, 33]}, {"text": "Corona", "indices": [103, 110]}, {"text": "Steuerschätzung", "indices": [258, 274]}], "symbols": [], "user_mentions": [{"screen_name": "OlafScholz", "name": "Olaf Scholz", "id": 38150247, "id_str": "38150247", "indices": [225, 236]}, {"screen_name": "spdbt", "name": "SPD-Fraktion im Bundestag", "id": 347792540, "id_str": "347792540", "indices": [237, 243]}, {"screen_name": "stimmeonline", "name": "Stimme.de", "id": 26986638, "id_str": "26986638", "indices": [244, 257]}], "urls": []}, "source": "<a href=\"http://twitter.com/download/android\" rel=\"nofollow\">Twitter for Android</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 738731811380076544, "user_id_str": "738731811380076544", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 0, "favorite_count": 1, "reply_count": 0, "quote_count": 0, "conversation_id": 1326936924393779201, "conversation_id_str": "1326936924393779201", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}, {"id_": "1329055795934322689", "raw_data": {"created_at": "Wed Nov 18 13:36:09 +0000 2020", "id": 1329055795934322689, "id_str": "1329055795934322689", "full_text": "Das Demonstrationsrecht ist ein wichtiges #Grundrecht unserer Verfassung, welches ich respektiere. Doch: Ich wünsche mir mehr #Respekt der Demonstrierenden vor dem Amt, der legitim gewählten Repräsentanten der Bürgerinnen und Bürger. \n#Infektionsschutzgesetz\n@spdbt @stimmeonline", "truncated": false, "display_text_range": [0, 279], "entities": {"hashtags": [{"text": "Grundrecht", "indices": [42, 53]}, {"text": "Respekt", "indices": [126, 134]}, {"text": "Infektionsschutzgesetz", "indices": [235, 258]}], "symbols": [], "user_mentions": [{"screen_name": "spdbt", "name": "SPD-Fraktion im Bundestag", "id": 347792540, "id_str": "347792540", "indices": [259, 265]}, {"screen_name": "stimmeonline", "name": "Stimme.de", "id": 26986638, "id_str": "26986638", "indices": [266, 279]}], "urls": []}, "source": "<a href=\"http://twitter.com/download/android\" rel=\"nofollow\">Twitter for Android</a>", "in_reply_to_status_id": null, "in_reply_to_status_id_str": null, "in_reply_to_user_id": null, "in_reply_to_user_id_str": null, "in_reply_to_screen_name": null, "user_id": 738731811380076544, "user_id_str": "738731811380076544", "geo": null, "coordinates": null, "place": null, "contributors": null, "is_quote_status": false, "retweet_count": 0, "favorite_count": 4, "reply_count": 0, "quote_count": 0, "conversation_id": 1329055795934322689, "conversation_id_str": "1329055795934322689", "favorited": false, "retweeted": false, "lang": "de", "supplemental_language": null}}]